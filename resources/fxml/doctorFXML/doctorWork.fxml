<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="grid" hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@/css/doctorWork.css" />
    </stylesheets>
    <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="369.8" minWidth="10.0" prefWidth="336.8" />
      <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints />
      <RowConstraints minHeight="10.0" />
      <RowConstraints />
      <RowConstraints />
  </rowConstraints>
   <children>
      <TextField fx:id="umbgF" GridPane.rowIndex="1" />
      <Label fx:id="label" maxWidth="1.7976931348623157E308" prefHeight="18.0" prefWidth="417.0" text="Enter the patient's umbg whose information you want to know:" GridPane.columnSpan="2" />
    <Button fx:id="submit" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#submit" text="Submit" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TableView GridPane.columnSpan="2" GridPane.rowIndex="3">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Label prefHeight="18.0" prefWidth="138.0" text="Waiting list:" GridPane.rowIndex="2">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Button fx:id="logout" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#logout" text="Logout" GridPane.columnIndex="1" GridPane.rowIndex="4" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
